/*
 * @file
 * Clientside validation install file
 */

/**
 * Implements hook_install
 */
function clientside_validation_enable() {
  //support for module like e.g. select_or_other
  db_update('system')
    ->fields(array(
      'weight' => 5,
    ))
    ->condition('type', 'module')
    ->condition('name', 'clientside_validation')
    ->execute();
}